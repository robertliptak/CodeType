{
  "name": "codetype",
  "version": "1.0.0",
  "description": "CodeType is a typing test application, made by me, Robert. The main aim of this app is to provide modern design for programmers wanting to improve their typing speed and accuracy. Not only coders but also ordinary users are able to enjoy typing tests with optional modes.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "client": "cd client && npm run dev",
    "server": "cd server && npm run dev",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertliptak/CodeType.git"
  },
